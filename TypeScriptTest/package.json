{
  "name": "typescript-node-mocha-template",
  "version": "1.0.0",
  "description": "A tempalte for a Kate with TypeScript, Node and mocha",
  "main": "src/serverConsolidation.ts",
  "scripts": {
    "clean": "rimraf ./dist",
    "precompile": "npm run clean",
    "compile": "tsc",
    "tsc": "tsc",
    "test": "mocha ./src/**/*.spec.ts --require ts-node/register --colors",
    "tdd": "mocha ./src/**/*.spec.ts -w --compilers ts:ts-node/register,tsx:ts-node/register --colors"
  },
  "author": "Maximiian Lackaw",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.0",
    "chai": "^4.1.2",
    "mocha": "^5.0.0",
    "rimraf": "^2.6.2",
    "ts-node": "^4.1.0",
    "typescript": "^2.7.1"
  }
}
